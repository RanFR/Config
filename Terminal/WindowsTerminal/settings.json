{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "splitPane",
        "split": "down",
        "splitMode": "duplicate"
      },
      "id": "User.splitPane.Vertical"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "right",
        "splitMode": "duplicate"
      },
      "id": "User.splitPane.Horizontal"
    },
    {
      "command": {
        "action": "sendInput",
        "input": "\u001b\n"
      },
      "id": "User.sendInput.WezTermLineBreak"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": false,
  "keybindings": [
    {
      "id": "Terminal.CopyToClipboard",
      "keys": "ctrl+c"
    },
    {
      "id": "Terminal.FindText",
      "keys": "ctrl+shift+f"
    },
    {
      "id": "Terminal.PasteFromClipboard",
      "keys": "ctrl+v"
    },
    {
      "id": "Terminal.DuplicatePaneAuto",
      "keys": "alt+shift+d"
    },
    {
      "id": "Terminal.SwitchToTab5",
      "keys": "alt+6"
    },
    {
      "id": null,
      "keys": "ctrl+alt+1"
    },
    {
      "id": "Terminal.SwitchToTab0",
      "keys": "alt+1"
    },
    {
      "id": "Terminal.SwitchToTab1",
      "keys": "alt+2"
    },
    {
      "id": null,
      "keys": "ctrl+alt+9"
    },
    {
      "id": null,
      "keys": "ctrl+alt+2"
    },
    {
      "id": "Terminal.SwitchToTab2",
      "keys": "alt+3"
    },
    {
      "id": null,
      "keys": "ctrl+alt+3"
    },
    {
      "id": "Terminal.SwitchToTab3",
      "keys": "alt+4"
    },
    {
      "id": "User.splitPane.Vertical",
      "keys": "alt+minus"
    },
    {
      "id": null,
      "keys": "ctrl+alt+7"
    },
    {
      "id": null,
      "keys": "ctrl+alt+4"
    },
    {
      "id": "Terminal.SwitchToTab4",
      "keys": "alt+5"
    },
    {
      "id": null,
      "keys": "ctrl+alt+5"
    },
    {
      "id": null,
      "keys": "ctrl+alt+6"
    },
    {
      "id": "Terminal.SwitchToTab6",
      "keys": "alt+7"
    },
    {
      "id": "Terminal.SwitchToTab7",
      "keys": "alt+8"
    },
    {
      "id": null,
      "keys": "ctrl+alt+8"
    },
    {
      "id": "Terminal.SwitchToLastTab",
      "keys": "alt+9"
    },
    {
      "id": "User.splitPane.Horizontal",
      "keys": "alt+plus"
    },
    {
      "id": "Terminal.ClosePane",
      "keys": "ctrl+w"
    },
    {
      "id": null,
      "keys": "ctrl+shift+w"
    },
    {
      "id": "User.sendInput.WezTermLineBreak",
      "keys": "shift+enter"
    }
  ],
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "profiles": {
    "defaults": {
      "colorScheme": "Catppuccin Mocha",
      "font": {
        "face": "Maple Mono NF CN"
      }
    }
  },
  "schemes": [
    {
      "background": "#1E1E2E",
      "black": "#45475A",
      "blue": "#89B4FA",
      "brightBlack": "#585B70",
      "brightBlue": "#89B4FA",
      "brightCyan": "#94E2D5",
      "brightGreen": "#A6E3A1",
      "brightPurple": "#F5C2E7",
      "brightRed": "#F38BA8",
      "brightWhite": "#A6ADC8",
      "brightYellow": "#F9E2AF",
      "cursorColor": "#F5E0DC",
      "cyan": "#94E2D5",
      "foreground": "#CDD6F4",
      "green": "#A6E3A1",
      "name": "Catppuccin Mocha",
      "purple": "#F5C2E7",
      "red": "#F38BA8",
      "selectionBackground": "#585B70",
      "white": "#BAC2DE",
      "yellow": "#F9E2AF"
    }
  ],
  "themes": [
    {
      "name": "Catppuccin Mocha",
      "tab": {
        "background": "#1E1E2EFF",
        "iconStyle": "default",
        "showCloseButton": "always",
        "unfocusedBackground": null
      },
      "tabRow": {
        "background": "#181825FF",
        "unfocusedBackground": "#11111BFF"
      },
      "window": {
        "applicationTheme": "dark",
        "experimental.rainbowFrame": false,
        "frame": null,
        "unfocusedFrame": null,
        "useMica": false
      }
    }
  ]
}
